{
  "name": "channelify",
  "image": "node:22.12.0-slim",
  "workspaceMount": "source=${localWorkspaceFolder},target=/channelify,type=bind,consistency=cached",
  "workspaceFolder": "/channelify",
  "remoteUser": "node",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": ["mhutchie.git-graph"],
      "settings": {
        "editor.tabSize": 2,
        "editor.detectIndentation": false
      }
    }
  }
}